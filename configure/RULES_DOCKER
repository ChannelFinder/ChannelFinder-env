.PHONY: install.docker build.docker prune.docker

install.docker:
	$(QUIET)$(INSTALL) -d $(INSTALL_LOCATION)
	$(QUIET)$(INSTALL) -m 744 $(SRC_PATH)/target/$(CFJAR_FILENAME)  $(INSTALL_LOCATION)/

build.docker:
	bash docker/scripts/docker_builder

prune.docker:
	$(QUIET)docker system prune --force
	$(QUIET)yes | docker image prune
