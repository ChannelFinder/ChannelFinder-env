[Unit]
Description=ChannelFinder Service
Documentation=https://github.com/ChannelFinder/ChannelFinder-SpringBoot
After=network.target elasticsearch.service
Requires=elasticsearch.service

[Service]
EnvironmentFile=_CFPATH_/cf.conf

# User=elasticsearch
# Group=elasticsearch

ExecStart=_JAVAPATH_/java $JAVA_OPTS -jar _CFPATH_/_CHANNELFINDER_JAR_NAME_
SuccessExitStatus=143 

ExecReload=kill -SIGINT $MAINPID
KillMode=process

Restart=on-failure
RestartSec=10s

[Install]
WantedBy=multi-user.target
Alias=cf.service